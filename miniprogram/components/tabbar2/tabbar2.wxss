.body{
    width: 100%;
    background-color: #f4f5f7;
    height: 150rpx;
    border-top: 1px solid #bfbfbf;
}
.body .tab1, .body .tab2, .body .tab3, .body .tab4{
    display: inline-block;
    width: 25%;
    height: 100%;
    font-weight: 540;
    line-height: 150rpx;
    text-align: center;
}
.tab1 image,.tab2 image,.tab3 image,.tab4 image{
    height: 32px;
    width: 32px;
    float: left;
    margin-top: 20rpx;
    margin-left: 63rpx;
}
.tab1 view,.tab2 view,.tab3 view,.tab4 view{
    display: inline-block;
    width: 100%;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 30rpx;
    float: left;
    margin-top: 17rpx;
    color: #8a8a8a;
}
.body .click{
    /*color: #24da6f;*/
    color: #ffcc33;
}