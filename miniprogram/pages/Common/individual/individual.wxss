.photoImage {
    width: 200rpx;
    height: 201rpx;
    position: relative;
    top: 30rpx;
    left: 12%;
    background: red;
    border-radius: 10%;
    margin-bottom: 50rpx;
    -moz-box-shadow: 3px 3px 4px rgba(0, 0, 0, 0.548);
    -webkit-box-shadow: 3px 3px 4px rgba(53, 50, 50, 0.76);
    box-shadow: 3px 3px 4px rgb(78, 77, 77);
    filter: progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=135, Color='#000000');
}
.money{
    position: relative;
    top: -250rpx;
    left: 50%;
    width: 200rpx;
    height: 150rpx;
}
.money>text{
    font-size: 50rpx;
    color: white;
    font-weight: 700;
}
.detail{
position: relative;
top:100rpx;
color: white;
font-weight: 700;
}
text{
    font-size: 30rpx;
    font-weight: 400;
}

.inputName {
    border-style: solid;
    border-color: black;
    border-radius: 20rpx;
    height: 70rpx
}


button {
    width: 180rpx;
    height: 80rpx;
    font-size: 30rpx;
    background-color: #4075e6;
    color: aliceblue;
    font-weight: 400;
}

.row {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
    /* 字体所居位置 align-item */
    position: relative;
    padding: 0 30rpx;
    height: 120rpx;
    width: 88%;
}

.name {
    position: relative;
    left: 130rpx;
}
.withdrawMoney{
    position: relative;
    top: 8%;
}

.withdraw {
    position: relative;
    left:470rpx;
}
.content{
    background-color: #e84118;
    width: 100%;
    height: 400rpx;
    /* shadow */
    -moz-box-shadow: 3px 3px 4px rgba(0, 0, 0, 0.548);
    -webkit-box-shadow: 3px 3px 4px rgba(53, 50, 50, 0.76);
    box-shadow: 3px 3px 4px rgb(78, 77, 77);
    filter: progid:DXImageTransform.Microsoft.Shadow(Strength=4, Direction=135, Color='#000000');

}
.changePhoto{
    position: relative;
    left: -25%;
}
page{
    background-color: #4b4b4b;
    height: 100%;
    
}
.txt{
    text-align: right;
    position: relative;
    left:30%;
}